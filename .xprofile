#! /bin/sh

if [ -d /etc/X11/xinit/xinitrc.d ]; then
    for f in /etc/X11/xinit/xinitrc.d/*; do
        [ -x "$f" ] && . "$f"
    done
    unset f
fi

export DE="xfce"
export EDITOR="nvim"

xset -dpms &
xset s off &
xset +fp /usr/share/fonts/local &
xset fp rehash &
xset r rate 200 30 &
setxkbmap -option compose:menu &

